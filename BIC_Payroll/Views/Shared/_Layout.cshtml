    <script>
        $(document).ready(function () {
            $('.minifyme').click(); 
            // DO NOT REMOVE : GLOBAL FUNCTIONS!
            pageSetUp();
            setInterval("settime()", 1000);
            onMenuLoad();
            getList();
            setDayDate();
        });

        function onMenuLoad() {
            var url = window.location.pathname;
            $(".clsMenu").removeClass('active');
            var res = url.substring(1, 8); //Remove this code once all pagename and controller name are updated
            if (res != 'Default') {
                $("#ulMenu").find('li').each(function () {
                    if ($(this).find('a').attr('href') == url) {
                        $(this).addClass("active");
                    }
                });
            }
        }
        
        function setDayDate() {
             
            var d = new Date();
            var weekday = new Array(7);
            weekday[0] = "Sunday";
            weekday[1] = "Monday";
            weekday[2] = "Tuesday";
            weekday[3] = "Wednesday";
            weekday[4] = "Thursday";
            weekday[5] = "Friday";
            weekday[6] = "Saturday";

            var n = weekday[d.getDay()]; 
            formatDate = (d.getMonth() + 1) + '/' + 
                         d.getDate() + '/' + 
                         d.getFullYear(); 
            var date = n+' - '+formatDate

            $("#dayDate").html(date);
        }



        function settime() {
             
            var dateTime = new Date();
            var hour = dateTime.getHours();
            var minute = dateTime.getMinutes();
            var second = dateTime.getSeconds();

            if (minute < 10)
                minute = "0" + minute;

            if (second < 10)
                second = "0" + second;

            var time = "" + hour + ":" + minute + ":" + second;

            $("#clock").html(time);
        }

        function getList() {
            var id = @SessLogObj.USERID;
            var compId=@SessLogObj.objComp.COMPID;
            var url='/Company/CompanyList'
            $.get(url,function (data) {
                var lidata='';
                var active='';
                $.each(data,function (index,value) {
                    if (compId === value.COMPID) {
                        active='active';
                    }else
                    {
                        active='';
                    }
                    lidata=lidata+'<li class="'+active+'"><a href="/Home/Index?Id='+value.COMPID+'"> '+value.COMPCODE+'</a></li>';
                });  
                $('#CompList').html(lidata)
            });
        }
    </script>

